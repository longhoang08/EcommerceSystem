<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobile_ui">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".SettingAccountActivity"
            android:label="@string/title_activity_setting_account"></activity>
        <activity android:name=".StallDetailProductActivity" />

        <activity android:name=".ProductSearchActivity"></activity>

        <activity android:name=".SearchActivity" />
        <activity android:name=".BuyRecordActivity" />
        <activity android:name=".AddProductActivity" />
        <activity android:name=".StallActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".LoginActivity" />
<!--        <activity android:name=".View.LoginActivity" />-->
        <activity android:name=".CartProductActivity" />
        <activity android:name=".ListReviewProductActivity" />
        <activity android:name=".DetailProductActivity" />
        <activity android:name=".ListCategoryActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>